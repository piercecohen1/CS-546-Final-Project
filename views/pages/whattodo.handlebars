
<section>
  <div class="main">
      <p>
        <h1>Find out what to do on your next vacation</h1>
        <h2>Read about interesting activities and attractions for your getaway.</h2>
      </p>
  </div>

  {{#each posts}}
    <article class="post">
      <h2><a href="/posts/{{_id}}">{{title}}</a></h2>
      <p>{{body}}</p>
    </article>
  {{/each}}
</section>
